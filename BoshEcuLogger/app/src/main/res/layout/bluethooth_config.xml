<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/phone_1136x640"
    android:paddingTop="10.0dip"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:textSize="25.0sp"
        android:textColor="#004986"
        android:id="@+id/no_data_view"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true" />
    <ExpandableListView
        android:id="@+id/list"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:cacheColorHint="#000000"
        android:childDivider="@drawable/line"
        android:divider="@drawable/line"
        android:dividerHeight="1.0dip"
        android:layout_above="@+id/SearchActivity_Search_button" />
    <Button
        android:textColor="@drawable/fault_btn_text_selector"
        android:id="@+id/SearchActivity_Search_button"
        android:background="#d4e4f3"
        android:layout_width="fill_parent"
        android:layout_height="40.0dip"
        android:layout_alignParentBottom="true" />
</RelativeLayout>